{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": [
                "dist/**"
            ]
        },
        "clean": {
            "dependsOn": [
                "^clean"
            ]
        },
        "check": {
            "dependsOn": [
                "^check"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}